extends layout

block content

  nav(class="w-100")
    ol(class="breadcrumb align-items-center")
      li(class="breadcrumb-item")
        a(href="/")
          =`Home`
      li(class="breadcrumb-item active")
        =`Tasks`

  table(class="table table-striped table-sm")
    tbody
      each task, i in tasks
        tr
          td= task.id
          td= task.title
